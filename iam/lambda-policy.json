{
    "Version": "2012-10-17",
    "Statement": [
	{
	
  	  "Effect": "Allow",
          "Action": "sts:AssumeRole",
          "Resource": "*"
	},	
        {
            "Effect": "Allow",
            "Action": "logs:CreateLogGroup",
            "Resource": "arn:aws:logs:*:254357752139:*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:logs:*:254357752139:log-group:/aws/lambda/*",
                "arn:aws:logs:*:254357752139:log-group:/aws/lambda/*"
            ]
        }
    ]
}